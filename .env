#Either native or git and if left empty, it will default to native
CONFIG_PROFILE=native

#If config_profile is git, then below Git configurations are required
#Git Repo URL
GIT_URI=

#Git Username
GIT_USERNAME=

#Git access token with read access to the repository
GIT_TOKEN=

#PSQL Credentials
#PSQL URL
PSQL_URL=jdbc:postgresql://localhost:5432/e_commerce

#PSQL Username
PSQL_USERNAME=

#PSQL Password
PSQL_PASSWORD=

#Keycloak Credentials
KEYCLOAK_URL=http://0.0.0.0:8080/
KEYCLOAK_REALM=user-realm
KEYCLOAK_CLIENT_ID=manav
#Client Secret Copy from Keycloak manav Client
KEYCLOAK_CLIENT_SECRET=
#Keycloak Customer group ID
KEYCLOAK_GROUP_ID=
#Keycloak Admin Group ID
KEYCLOAK_ADMIN_GROUP_ID=

#Keycloak Database Credentials
POSTGRES_KEYCLOAK_USER=
POSTGRES_KEYCLOAK_PASSWORD=

#Keycloak Admin Credentials
KEYCLOAK_ADMIN_USERNAME=
KEYCLOAK_ADMIN_PASSWORD=